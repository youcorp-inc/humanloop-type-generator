{
  "name": "@boardy/humanloop-type-generator",
  "version": "1.1.0",
  "description": "Strongly typed wrapper for the Humanloop SDK that preserves folder structure and generates TypeScript interfaces",
  "main": "dist/index.js",
  "bin": {
    "humanloop": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/cli.ts",
    "test": "ts-node src/test.ts",
    "example": "ts-node examples/usage.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "humanloop",
    "typescript",
    "types",
    "generation",
    "ai",
    "llm",
    "prompt",
    "sdk",
    "client"
  ],
  "author": "Abhinav Boyed",
  "homepage": "https://github.com/abhinav/humanloop-type-generator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhinav/humanloop-type-generator.git"
  },
  "license": "MIT",
  "dependencies": {
    "humanloop": "^0.8.15",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.19.79",
    "dotenv": "^16.4.7",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
